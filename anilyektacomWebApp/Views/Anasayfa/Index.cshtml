@using System.Data;
@{
    Layout = "~/Views/Shared/_Layout_AdminLTE.cshtml";
    ViewData["Title"] = "Anasayfa";

    
    string BugunkiBagliKisiler = "100"; 
    string GenelBagliKisiler = "100";

    string BugunkiVeriSayisi = "100"; 
    string GenelVeriSayisi = "100";

    
    int UyariGecikme = 20;
    int UyariIcaRttMS = 20;
    int AnaSayfaUyariYenileme = 20;
    int AnaSayfaUyariSonSaat = 20;
}
<!-- Content Wrapper. Contains page content -->
<div class="content-wrapper">
    <section class="content">
        <div class="content-header">
            <!-- Info boxes -->
            <div class="row">
                <!-- fix for small devices only -->
                <div class="clearfix hidden-md-up"></div>

                <div class="col-12 col-sm-6 col-md-4">
                    <div class="info-box">
                        <span class="info-box-icon bg-maroon elevation-1"><i class="fas fa-cog"></i></span>

                        <div class="info-box-content">
                            <span class="info-box-text">Uyarı kabul edilen gecikme değeri @UyariGecikme.ToString()</span>
                            <span class="info-box-text">Uyarı kabul edilen IcaRttMS değeri @UyariIcaRttMS.ToString()</span>
                        </div>
                        <!-- /.info-box-content  -->
                    </div>
                    <!-- /.info-box -->
                </div>

                <div class="col-12 col-sm-6 col-md-4">
                    <div class="info-box mb-3">
                        <span class="info-box-icon bg-success elevation-1"><i class="fas fa-solid fa-database"></i></span>

                        <div class="info-box-content">
                            <span class="info-box-text">Bugünki toplam veri sayısı @BugunkiVeriSayisi</span>
                            <span class="info-box-text">Toplam veri sayısı @GenelVeriSayisi</span>
                        </div>
                        <!-- /.info-box-content -->
                    </div>
                    <!-- /.info-box -->
                </div>
                <!-- /.col -->
                <div class="col-12 col-sm-6 col-md-4">
                    <div class="info-box">
                        <span class="info-box-icon bg-info elevation-1"><i class="fas fa-users"></i></span>

                        <div class="info-box-content">
                            <span class="info-box-text">Bugünki toplam kullanıcı sayısı @BugunkiBagliKisiler</span>
                            <span class="info-box-text">Toplam kullanıcı sayısı @GenelBagliKisiler</span>
                        </div>
                        <!-- /.info-box-content -->
                    </div>
                    <!-- /.info-box -->
                </div>

                <!-- /.col -->
            </div>
            <!-- /.row -->

            <div class="row">
                <!-- fix for small devices only -->
                <div class="clearfix hidden-md-up"></div>

                <!-- DIRECT CHAT -->
                <div class="col-12">

                    <div class="card direct-chat direct-chat-primary">

                        <div class="card-header">
                            <h3 class="card-title">Son @AnaSayfaUyariSonSaat.ToString() saat içerisinde bağlantısı uyarı değerinin üzerine çıkanların listesi <i>(@AnaSayfaUyariYenileme.ToString() dakikada bir yenilenmektedir)</i></h3>
                            <div class="card-tools">
                                <button type="button" class="btn btn-tool" data-card-widget="remove" onClick="Durdur()">
                                    <i class="fas fa-times"></i>
                                </button>
                            </div>
                        </div>
                        <!-- /.card-header -->

                        <div class="card-body bekleniyor">
                            <p id="bekleniyor"></p>
                            <p id="icerik"></p>
                        </div>
                    </div>
                    <!-- /.row -->
                </div><!--/. container-fluid -->
            </div>
    </section>
    <!-- /.content -->
</div>
<!-- /.content-wrapper -->
<!-- REQUIRED SCRIPTS -->
<!-- jQuery -->
<script src="~/Content/plugins/jquery/jquery.min.js"></script>
<!-- Bootstrap -->
<script src="~/Content/plugins/bootstrap/js/bootstrap.bundle.min.js"></script>
<!-- overlayScrollbars -->
<script src="~/Content/plugins/overlayScrollbars/js/jquery.overlayScrollbars.min.js"></script>
<!-- AdminLTE App -->
<script src="~/Content/dist/js/adminlte.js"></script>

<!-- PAGE PLUGINS -->
<!-- jQuery Mapael -->
<script src="~/Content/plugins/jquery-mousewheel/jquery.mousewheel.js"></script>
<script src="~/Content/plugins/raphael/raphael.min.js"></script>
<script src="~/Content/plugins/jquery-mapael/jquery.mapael.min.js"></script>
<script src="~/Content/plugins/jquery-mapael/maps/usa_states.min.js"></script>
<!-- ChartJS -->
<script src="~/Content/plugins/chart.js/Chart.min.js"></script>


<script src="~/Content/plugins/chart.js/Chart.min.js"></script>
